// gcd means greatest comman divisior
#include <iostream>
using namespace std;
int main() {
    int n1,n2;
    cout<<"enter the two number:"<<endl;
    cin>>n1>>n2;
    while(n1!=n2){
        if(n1>n2){
            n1-=n2;
        }
        else{
            n2-=n1;
        }
    }
    cout<<"gcd of two number is ="<<n1<<endl;
    return 0;
}
