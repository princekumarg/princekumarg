#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<=n;i++){
        cin>>arr[i];
    }
    int x;
    cin>>x;
    bool pre=false;
    for(int i=0;i<=n;i++){
        if(arr[i]==x){
            pre=true;
            break;
        }
        
    }
    if(pre==true){
        cout<<x<<"is present in array ";
    }
    else{
        cout<<x<<"is not present in array ";
    }
    return 0;
}
