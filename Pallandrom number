#include <iostream>  
using namespace std;  
int main()  
{  
  int n,r,rev=0,temp;    
  cout<<"Enter the Number=";    
  cin>>n;    
 temp=n;    
 while(n>0)    
{    
 r=n%10;    
 rev=rev*10+r;    
 n=n/10;    
}    
if(temp==rev)    
cout<<"Number is Palindrome."<<rev;  
else    
cout<<"Number is not Palindrome."<<rev;  
  return 0;  
}
