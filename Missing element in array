#include<iostream>
using namespace std;
int main ()
{
    int arr[10], n, i, sum = 0,total;
    total=(n+1)*(n+2)/2;
    cout << "Enter the size of the array : ";
    cin >> n;
    cout << "\nEnter the elements of the array : ";
    for (i = 0; i < n; i++)
    cin >> arr[i];
    for (i = 0; i < n; i++)
    {
        sum += arr[i];
    }
    cout << "\nthe missing element in array is: " << total-sum;
    return 0;
}
