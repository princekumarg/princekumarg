#include <iostream>
using namespace std;
int main() {
  char op;
  int a,b;
  cout<<"enter the op as(+, -, *, /) ";
  cin>>op;
  cout<<"enter the two number";
  cin>>a>>b;
  switch(op){
      case'+':
        cout<<a<<"+"<<b<<"="<<a+b;
        break;
      case'-':
        cout<<a<<"-"<<b<<"="<<a-b;
        break;
      case'*':
        cout<<a<<"*"<<b<<"="<<a*b;
        break;
      case'/':
        cout<<a<<"/"<<b<<"="<<a/b;
        break;
      default:
        cout<<"error";
        break;
  }
  return 0;
}
